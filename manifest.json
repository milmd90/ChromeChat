{
    "manifest_version": 2,
    "name": "Chrome Extension",
    "version": "1.0.0",
    "permissions": [
        "http://*/"
    ],
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [
                "<all_urls>"
            ],
            "js": ["public/bundle.js"]
        }
    ]
}
